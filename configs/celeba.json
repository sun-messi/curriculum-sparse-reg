{
  "dataset": "celeba",
  "diffusion": {
    "timesteps": 1000,
    "beta_start": 0.0001,
    "beta_end": 0.02,
    "beta_schedule": "linear",
    "model_mean_type": "eps",
    "model_var_type": "fixed-small",
    "loss_type": "mse"
  },
  "model": {
    "in_channels": 3,
    "hid_channels": 128,
    "ch_multipliers": [1, 2, 2, 2],
    "num_res_blocks": 2,
    "apply_attn": [false, false, true, false],
    "drop_rate": 0.0
  },
  "train": {
    "lr": 2e-5,
    "batch_size": 128,
    "grad_norm": 1.0,
    "epochs": 20,
    "warmup": 5000,
    "use_ema": true,
    "ema_decay": 0.9999,
    "num_samples": 12,
    "image_intv": 1,
    "chkpt_intv": 1
  },
  "curriculum": {
    "enabled": false
  },
  "sparsity": {
    "enabled": false,
    "initial_sparsity": 0.8,
    "regrowth_method": "gradient"
  },
  "regularization": {
    "enabled": false,
    "lambda_max": 0.00003
  }
}